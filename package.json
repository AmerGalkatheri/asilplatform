{
  "name": "obar-platform",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run -w apps/web dev",
    "dev:backend": "npm run -w apps/backend dev",
    "dev:web": "npm run -w apps/web dev",
    "build": "npm run -w apps/backend build && npm run -w apps/web build",
    "start": "npm run -w apps/backend start && npm run -w apps/web start"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
